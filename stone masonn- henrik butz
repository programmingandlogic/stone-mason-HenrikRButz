/**
 * StoneMasonKarel.java
 **/
public void run (){
	BeeperUp();
	MoveUp();
	while (frontIsclear)) {
		beeperUp();
		MoveUp();
		BeeperDown();
		MoveDown();
		
	}
}
private void beeperUp(){
	TurnLeft();
	while (FrontIsClear)) {
		if (noBeepersPresent)) {
			putBeeper();
		
			move();
		}
	}
private void BeeperDown(){
	turnRight();
	while (frontIsClear)) {
		if (noBeepersPresent)) {
			putBeeper();
		}
		move();
	} 
}
private void BeeperDwon() {
	turnRight();
	while (frontIsClear)){
		if (noBeepersPresent)){
			putBeeper();
		}
		move();
	}
}
